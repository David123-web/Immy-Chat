{
  "name": "immersio-backend",
  "version": "0.0.1",
  "description": "Backend for Immersio",
  "author": "UTeam",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && npx prisma generate",
    "clean": "rm -rf node_modules && rm -rf dist && yarn",
    "build": "nest build && cp -R ./client ./dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db": "npx prisma generate && npx prisma db push",
    "start:dev": "nest start --watch && cp -R ./client ./dist/",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "set TS_JEST_DISABLE_VER_CHECKER=true && jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/local-auth": "2.1.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/throttler": "^3.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.4.2",
    "@sendgrid/mail": "^7.7.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "cuid": "^3.0.0",
    "dayjs": "^1.11.6",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.3.0",
    "googleapis": "105",
    "helmet": "^6.0.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.1",
    "kysely": "^0.26.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nanoid": "3.3.4",
    "nestjs-kysely": "^0.1.6",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-magic-link": "^2.1.0",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^8.11.3",
    "prisma": "^5.4.2",
    "prisma-kysely": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.2",
    "slugify": "^1.6.6",
    "smtp-client": "^0.4.0",
    "stripe": "^11.4.0",
    "twilio": "^3.83.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/testing": "^9.0.0",
    "@types/archiver": "^5.3.1",
    "@types/aws-lambda": "^8.10.59",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "aws-sdk": "^2.1263.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^6.9.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
